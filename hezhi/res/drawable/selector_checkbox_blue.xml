<?xml version="1.0" encoding="utf-8"?>
<!-- viewpager引导的左侧选择器（半圆角） -->
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_checked="true"><shape android:shape="rectangle">
            <solid android:color="@color/white" />
            <corners android:bottomLeftRadius="@dimen/circlecornners2" android:topLeftRadius="@dimen/circlecornners2"  android:bottomRightRadius="@dimen/circlecornners2" android:topRightRadius="@dimen/circlecornners2"/>
        </shape></item>
    <item><layer-list>
            <item><shape android:shape="rectangle">
                    <solid android:color="@color/white" />

                    <corners android:bottomLeftRadius="@dimen/circlecornners2" android:topLeftRadius="@dimen/circlecornners2" android:bottomRightRadius="@dimen/circlecornners2" android:topRightRadius="@dimen/circlecornners2"/>
                </shape></item>
            <item android:bottom="1dp" android:left="1dp" android:right="1dp" android:top="1dp"><shape android:shape="rectangle">
                    <solid android:color="@color/base" />

                    <corners android:bottomLeftRadius="@dimen/circlecornners2" android:topLeftRadius="@dimen/circlecornners2" android:bottomRightRadius="@dimen/circlecornners2" android:topRightRadius="@dimen/circlecornners2" />
                </shape></item>
        </layer-list></item>

</selector>